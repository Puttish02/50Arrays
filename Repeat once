package FiftyArrayQ;

public class Repeatonce {
    static void search(int arr[],int n){

        int xor = arr[0];

        for (int i=1;i<n;i++) {
            xor = xor^arr[i];
        }
        System.out.println("required element is"+xor);
    }

    public static void main(String[] args) {
        int arr[]={1,1,2,2,3,3,4,5,5,6,6};
        int n= arr.length;
        search(arr,n);
    }
}
