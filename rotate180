package FiftyArrayQ;

public class rotate180 {
    static void rotate180(int arr[][],int n){

        for (int i=n-1;i>=0;i--){
            for (int j=n-1;j>=0;j--)
                System.out.print(arr[i][j]+" ");
            System.out.println();
        }
    }

    public static void main(String[] args) {
        int arr[][]={{1,2,34,4},{5,6,7,8},{9,8,7,6},{45,6,4,3}};
        int n = arr.length;
        rotate180(arr,n);
    }
}
